/* Qr Container */

.qr-container {
    display: flex;
    flex-direction:column;
    width: 30%;
    height: 450px;
    background-color: #006eff;
    border-radius: 25px;
}
.qr-container .txt{
    color: black;
    font-size: 20px;
    align-self: center;
    margin-top: 10%;
}
.qr-container .txt h1{
    color: #000;
    font-size: 28px;
    margin-bottom: 10px;
    margin-left: 5%;
}
.qr-container .txt p{
    color: #292929;
    font-size: 15px;
    padding: 1.02 rem;
}
.qr-container .txt p a{
    text-decoration: none;
    color: #000;
    font-weight: bold;
}
.qr-container .qr {
    width: 280px;
    height: 280px;
    background-color: #fff;
    align-self: center;
    border-radius: 25px;
}
.qr-container .qr img {

    width:100%;
    height:100%;
    padding: 0.75rem;
    color:#006eff;
}
